{
    "version": 2,
    "builds": [
        { "src": "*.html", "use": "@vercel/static" },
        { "src": "./angular", "use": "@vercel/static" },
       { "src": "./index.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "handle": "filesystem" },
        { "src": "/(.*)", "dest": "/angular/index.html" },
       { "src": "/(.*)", "dest": "/" }
    ]
 }

 